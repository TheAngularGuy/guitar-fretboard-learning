{
  "hosting": {
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=432000"
          }
        ]
      }
    ],
    "public": "dist/guitar-fretboard-learning",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
